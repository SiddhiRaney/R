# -------------------------------------------
# 86. PARETO CHART
# Sorted bar chart with cumulative line

values <- c(5,3,9,6,2)
names(values) <- c("A","B","C","D","E")
values <- sort(values, decreasing=TRUE)
barplot(values, col="lightblue", main="Pareto Chart")
lines(cumsum(values)/sum(values)*max(values),
      type="b", col="red")

# -------------------------------------------
# 87. MIRROR BAR PLOT
