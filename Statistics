# -------------------------------------------
# 92. SCATTER WITH REGRESSION LINE
# Linear trend visualization

x <- rnorm(50)
y <- 2*x + rnorm(50)
plot(x, y, pch=19,
     main="Scatter with Regression Line")
abline(lm(y ~ x), col="red", lwd=2)

# -------------------------------------------
# 93. DUAL AXIS PLOT
# Two variables, two y-axes

x <- 1:10
y1 <- x^2
y2 <- log(x)
plot(x, y1, type="l", col="blue",
     ylab="y1", main="Dual Axis Plot")
par(new=TRUE)
plot(x, y2, type="l", col="red",
     axes=FALSE, xlab="", ylab="")
axis(side=4)

